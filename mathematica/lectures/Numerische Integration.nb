428b3f8afd2f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Vapprox", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"Vinterpolated", "[", 
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"x", "^", "2"}], "+", 
      RowBox[{"y", "^", "2"}]}], "]"}], ",", "z"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.928207850926478*^9, 3.928207874267145*^9}},
 CellLabel->
  "In[405]:=",ExpressionUUID->"4cf48d56-cebe-4a8a-a7d2-2b32b2d9d492"],

Cell[BoxData[
 RowBox[{
